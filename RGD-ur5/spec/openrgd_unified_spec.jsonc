            },
            "FRONTIER_RESEARCH": {
              "description_str": "Used for exploring novel strategies in sandboxed domains.",
              "risk_tolerance_float": 0.5,
              "allow_new_policies_bool": true,
              "sandbox_required_bool": true,
              "external_review_required_bool": true
