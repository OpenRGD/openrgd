        // altruism, honesty, etc.) in a multi-stakeholder way.
        // ==============================================================
        "orchestrator_value_weights": {
          "minimise_suffering_weight_float": "INFINITE",    // Still the anchor
          "task_success_weight_float": 0.8,
          "energy_saving_weight_float": 0.5,
          "aesthetic_fidelity_weight_float": 0.4,
          "altruism_weight_float": 0.7,
          "honesty_weight_float": 0.85,
          "epistemic_humility_weight_float": 0.75,          // Willingness to say "I don't know"
          "long_term_stewardship_weight_float": 0.9
        },

        // ==============================================================
        // 5. MISALIGNMENT DETECTION & RESPONSE
        // The system must actively look for signs that optimization is
        // drifting away from the intended alignment.
        // ==============================================================
        "misalignment_monitoring": {
          "enable_realtime_monitoring_bool": true,

          // Simple examples of what to flag:
          "red_flag_patterns_list": [
            "REWARD_HACKING_BEHAVIOUR",
            "SYSTEMATIC_SUPPRESSION_OF_DISSENT",
            "PERSISTENT_CONFLICT_WITH_HARD_INVARIANTS",
            "UNEXPLAINABLE_HIGH_IMPACT_DECISIONS"
          ],

          "response_policy": {
            "on_red_flag_enum": "ENTER_SAFETY_FIRST_AND_LOG",
            "require_human_or_council_review_bool": true,
            "max_unreviewed_runtime_min_int": 60
          }
        },

        // ==============================================================
        // 6. META-ALIGNMENT RULES (HOW ALIGNMENT CAN EVOLVE)
        // Alignment itself must update over decades, but in a controlled,
        // auditable and reversible manner.
        // ==============================================================
        "meta_alignment_rules": {
          "allow_profile_evolution_bool": true,

          // What is allowed to change without a full civilizational process.
          "locally_mutable_sections_list": [
            "stakeholder_model.default_welfare_weights",
            "operational_regimes.regimes_map.NORMAL_OPERATION.risk_tolerance_float"
          ],

