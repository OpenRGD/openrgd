          "recommended_refresh_interval_hours_int": 24,
          "used_by_components_list": [
            "03_agency/swarm_protocol.json",
            "04_volition/orchestra.json",
            "06_ether/consensus_reality.json",
            "06_ether/reputation_graph.json"
